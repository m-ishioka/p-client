{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "APP_ENV=development NODE_OPTIONS='--inspect' next dev -p 3001",
    "ssr-build": "next build",
    "development:ssg-build": "APP_ENV=development next build && next export",
    "production:ssg-build": "APP_ENV=production next build && next export",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "gen:services": "buf generate ./proto/entpb.proto"
  },
  "dependencies": {
    "@bufbuild/connect-web": "^0.6.0",
    "@bufbuild/protobuf": "^1.0.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@m-ishioka/p-protocol-buffuers": "^1.0.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.1",
    "@next/font": "13.1.1",
    "@tailwindcss/typography": "^0.5.8",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@vercel/analytics": "^0.1.6",
    "autoprefixer": "^10.4.13",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "eslint": "8.30.0",
    "eslint-config-next": "13.1.1",
    "fs": "0.0.1-security",
    "github-markdown-css": "^5.1.0",
    "google-protobuf": "^3.21.2",
    "gray-matter": "^4.0.3",
    "grpc": "^1.24.11",
    "grpc-web": "^1.4.2",
    "grpc-web-client": "^0.7.0",
    "next": "13.1.1",
    "next-auth": "^4.18.7",
    "next-http-proxy-middleware": "^1.2.5",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.20",
    "raw-loader": "^4.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.4",
    "recharts": "2.4.1",
    "swr": "^2.0.0",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-connect-web": "^0.6.0",
    "@bufbuild/protoc-gen-es": "^1.0.0",
